{
  "name": "System",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "MessageQueue": {
      "consumers": {
        "consumerStart": {
          "queue": "lbseed::start",
          "type": "fdsmedia.lbseed.job.start"
        }
      },
      "producers": {
        "producerNotification": {
          "exchange": "notification",
          "options": {
            "routingKey": "notification::log",
            "type": "peakssignals.job.log.notification"
          }
        },
        "producerError": {
          "exchange": "lbseed",
          "options": {
            "routingKey": "lbseed::error",
            "type": "fdsmedia.error"
          }
        }
      }
    }
  },
  "properties": {},
  "validations": [],
  "relations": {},
  "acls": [],
  "methods": {
    "generateQrCode": {
    "accepts": [
      {
        "arg": "email",
        "type": "string",
        "required": true
      },
      {
        "arg": "token",
        "type": "string",
        "required": true
      }
    ],
    "returns": [
      {
        "arg": "code",
        "type": "buffer",
        "root": false,
        "description": ""
      }
    ],
    "description": "",
    "http": {
      "verb": "get"
    }
  },
    "getQRCode": {
      "accepts": [{
        "arg": "email",
        "type": "string"
      }],
      "returns": [{
        "arg": "code",
        "type": "buffer",
        "root": false,
        "description": ""
      }],
      "description": "",
      "http": [
        {
          "path": "/getQrCode",
          "verb": "get"
        }
      ]
    },
    "getPublicQRCodePath": {
      "accepts": [{
        "arg": "email",
        "type": "string"
      }],
      "returns": [{
        "arg": "url",
        "type": "string",
        "root": false
      }],
      "description": "",
      "http": [
        {
          "path": "/getPublicQRCodePath",
          "verb": "get"
        }
      ]
    }
  }
}
